    {
     "cell_type": "code",
     "collapsed": false,
     "input": [
      "def mypi(n):\n",
      "   \n",
      "    nin = 0.\n",
      "    \n",
      "    for i in range(n):\n",
      "        x,y=np.random.rand(2)\n",
      "        \n",
      "        if x**2+y**2 < 1:\n",
      "            nin = nin + 1.0\n",
      "    \n",
      "    return 4*nin/n  \n",
      "\n",
      "mypi(1000000)"
     ],
