alias po='vim ~/.bash_profile ~/.bashrc ~/.bashinvoca ~/.bashprivate ~/.bash_az ~/.bash_aliases ~/.bash_git ~/.bash_qa ~/.bash_functions && source ~/.bash_profile && ~/.push_dot.sh' 
